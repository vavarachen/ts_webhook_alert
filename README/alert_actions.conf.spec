[ts_webhook_alert]
# Rest API endpoint to import indicators
param.ts_import_url = <string>

# Notes (tags). CSV.
param.ts_tags = <string>

# Indicator sharing group(s).  CSV.
param.ts_trusted_circles = <string>

# Source confidence
param.ts_source_confidence = <integer>

# Confidence
param.ts_confidence = <integer>

# Indicator classification
param.ts_classification = [private|public]

# Indicator severity
param.ts_severity = [low|medium|high|very-high]

# Indicator mappings
param.ts_mapping_md5 = <string>
param.ts_mapping_ip = <string>
param.ts_mapping_domain = <string>
param.ts_mapping_url = <string>
param.ts_mapping_email = <string>
